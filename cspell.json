{
    "version": "0.2",
    "dictionaryDefinitions": [
        {
            "name": "project-words",
            "path": "./custom-words.txt",
            "description": "Workspace level words",
            "addWords": true
        }
    ],
    "useGitignore": true,
    "ignorePaths": [
        ".git",
        ".gitattributes",
        ".gitignore",
        ".travis.yml",
        ".vscode",
        ".vscodeignore",
        "*.vsix",
        "**/templates/samples/**",
        "CHANGELOG.md",
        "extensions/*/samples/**",
        "node_modules",
        "package-lock.json",
        "generator-cspell-dicts-extensions/generators/app/templates/**",
        "static/generated"
    ],
    "dictionaries": [
        "project-words"
    ]
}
